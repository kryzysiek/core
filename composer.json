{
    "name": "landingi/core",
    "description": "Landingi Standard Library",
    "type": "library",
    "license": "proprietary",
    "require": {
        "php": "^7.3",
        "aws/aws-sdk-php": "^3.172",
        "ext-json": "*",
        "symfony/uid": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^0.12",
        "symplify/easy-coding-standard": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Landingi\\Core\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Landingi\\Core\\" : "tests"
        }
    },
    "minimum-stability": "stable"
}
